  "scripts": {
    "start": "babel-node server/server.js",
    "start:dev": "nodemon --exec \"babel-node server/server.js\"",
    "test": "mocha --compilers js:@babel/register test/index.js --coverage --exit",
    "test:dev": "mocha --compilers js:@babel/register --watch \"test/index.js\" --coverage --watch-extensions js",
    "cover": "nyc npm run test",
    "webpack:dev": "webpack-dev-server --mode development --hot",
    "webpack:build": "webpack --mode production",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test"
  },
